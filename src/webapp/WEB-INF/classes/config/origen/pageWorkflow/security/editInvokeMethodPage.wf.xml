                    <mapping id="task.subscriber" className="string">
                        <entries/>
                    </mapping>
                </mappings>
                <actions>
                    <action managerName="reflectMethodManager" methodName="invokeMethod" parameter="task.subscriber"/>
                </actions>
                <forwards>
                    <forward name="resetWindow" newEnabled="true" editEnabled="true"/>
                </forwards>
            </formCommit>
        </process>
    </main>
    <detail/>
</page>