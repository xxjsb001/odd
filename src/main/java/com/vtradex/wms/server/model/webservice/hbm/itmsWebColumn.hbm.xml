<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.vtradex.wms.server.model.webservice.ItmsWebColumn" table="Itms_web_Column" dynamic-update="true">
        <id name="id" column="ID" type="long">
            <generator class="native">
                <param name="sequence">iseq_ItmsWebColumn</param>
                <param name="parameters">START WITH 1</param>
            </generator>
        </id>
        <discriminator column="DISCRIMINATOR" type="string"/>
		<version name="version" column="VERSION" type="long"/>
		
		<many-to-one name="methods" class="com.vtradex.wms.server.model.webservice.ItmsWebArgument">
			<column name="web_argument_id" not-null="true" unique-key="UK_ITMSWEBCOLUMN"/>
		</many-to-one>
		<property name="jsonC" column="JSON_C" type="string" length="100"  not-null="true" unique-key="UK_ITMSWEBCOLUMN"/>
		<property name="jsonV" column="JSON_V" type="string" length="100"  not-null="true"/>
		<component name="updateInfo" class="com.vtradex.thorn.server.model.UpdateInfo">
			<property name="creatorId" type="long" column="CREATOR_ID"/>
			<property name="creator" type="string" length="50" column="CREATOR"/>
			<property name="createdTime" type="timestamp" column="CREATED_TIME"/>
			<property name="lastOperatorId" type="long" column="LAST_OPERATOR_ID"/>
			<property name="lastOperator" type="string" length="50" column="LAST_OPERATOR"/>
			<property name="updateTime" type="timestamp" column="UPDATE_TIME"/>
		</component>
        		
	</class>
</hibernate-mapping>