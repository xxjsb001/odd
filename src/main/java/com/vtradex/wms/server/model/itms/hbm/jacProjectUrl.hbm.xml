<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd"> 
<hibernate-mapping>
    <class name="com.vtradex.wms.server.model.itms.JacProjectUrl" table="ITMS_JACPROJECTURL" dynamic-update="true">
        <id name="id" column="ID" type="long">
            <generator class="native">
                <param name="sequence">wseq_jacProjectUrl</param>
                <param name="parameters">START WITH 1</param>
            </generator>
        </id> 
        <discriminator column="DISCRIMINATOR" type="string"/>
		<version name="version" column="VERSION" type="long"/>
		
		<property name="url" column="URL" type="string" length="100"/>
		<property name="description" column="DESCRIPTION" type="string" length="255"/>
		<property name="synonymName" type="string">
            <column name="SYNONYM_NAME" length="50"/>
        </property>
        <property name="sortName" type="string">
            <column name="SORT_NAME" length="50"/>
        </property>
		<component name="updateInfo" class="com.vtradex.thorn.server.model.UpdateInfo">
			<property name="creatorId" type="long" column="CREATOR_ID"/>
			<property name="creator" type="string" length="50" column="CREATOR"/>
			<property name="createdTime" type="timestamp" column="CREATED_TIME"/>
			<property name="lastOperatorId" type="long" column="LAST_OPERATOR_ID"/>
			<property name="lastOperator" type="string" length="50" column="LAST_OPERATOR"/>
			<property name="updateTime" type="timestamp" column="UPDATE_TIME"/>
		</component>
	</class>
</hibernate-mapping>